          "type": "dropShadow"
        }
      },
      "tertiary-hover": {
        "$type": "boxShadow",
        "$value": {
          "x": "{shadow.xpos.xs}",
          "y": "{shadow.ypos.xs-hover}",
          "blur": "0",
          "spread": "0",
          "color": "{color.boxShadow.button.mainSubtle}",
          "type": "dropShadow"
        }
      }
    },
    "borderRadius": {
      "sm": {
        "$type": "borderRadius",
        "$value": "{dimension.4}"

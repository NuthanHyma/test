  "shadow": {
    "xpos": {
      "xs": {
        "$type": "dimension",
        "$value": "0px"
      }
    },
    "ypos": {
      "xs": {
        "$type": "dimension",
        "$value": "6px"
      },
      "xs-hover": {
        "$type": "dimension",
        "$value": "4px"
      }
    }
  },
  "fontWeight": {
    "label": {
      "$type": "fontWeights",
      "$value": "{fontWeight.medium}"
    },
    "labelEmphasis": {
      "$type": "fontWeights",
      "$value": "{fontWeight.semiBold}"
    }
  },
  "button": {
    "elevation": {
      "primary": {
        "$type": "boxShadow",
        "$value": {
          "x": "{shadow.xpos.xs}",
          "y": "{shadow.ypos.xs}",
          "blur": "0",
          "spread": "0",
          "color": "{color.boxShadow.button.mainStrong}",
          "type": "dropShadow"
        }
      },
      "primary-hover": {
        "$type": "boxShadow",
        "$value": {
          "x": "{shadow.xpos.xs}",
          "y": "{shadow.ypos.xs-hover}",
          "blur": "0",
          "spread": "0",
          "color": "{color.boxShadow.button.mainStrong}",
          "type": "dropShadow"
        }
      },
      "secondary": {
        "$type": "boxShadow",
        "$value": {
          "x": "{shadow.xpos.xs}",
          "y": "{shadow.ypos.xs}",
          "blur": "0",
          "spread": "0",
          "color": "{color.boxShadow.button.main}",
          "type": "dropShadow"
        }
      },
      "secondary-hover": {
        "$type": "boxShadow",
        "$value": {
          "x": "{shadow.xpos.xs}",
          "y": "{shadow.ypos.xs-hover}",
          "blur": "0",
          "spread": "0",
          "color": "{color.boxShadow.button.main}",
          "type": "dropShadow"
        }
      },
      "tertiary": {
        "$type": "boxShadow",
        "$value": {
          "x": "{shadow.xpos.xs}",
          "y": "{shadow.ypos.xs}",
          "blur": "0",
          "spread": "0",
